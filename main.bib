  Publisher                = {Gabler [u.a.]},
  Reportid                 = {RWTH-CONV-079453},
  Review                   = {from: [Klocke-2016]},
  Timestamp                = {2017.12.07},
  Typ                      = {PUB:(DE-HGF)16},
  Url                      = {http://publications.rwth-aachen.de/record/217613}
}

