@Article{Dombrowski2009,
  Title                    = {Lean Ramp-up. Ein Organisationsmodell für den effizienten Serienanlauf in KMU},
  Author                   = {Dombrowski, Uwe and Hanke, Tobias},
  Journal                  = {ZWF Zeitschrift für wirtschaftlichen Fabrikbetrieb},
  Year                     = {2009},
  Note                     = {Dokumenttyp: Zeitschrift},
  Number                   = {10},
  Pages                    = {877-883},
  Volume                   = {104},

  Abstract                 = {Viele Unternehmen produzieren neue Produkte mit großer Variantenvielfalt in immer kürzeren Zeitabschnitten, sodass die Frequenz der Serienanläufe zunimmt und ein effizienter Serienanlauf immer mehr an Bedeutung gewinnt. In zahlreichen Forschungsprojekten wurden deshalb Methoden und Werkzeuge (Organisationsmodelle) zur Verbesserung des Serienanlaufs entwickelt, die jedoch primär auf die Bedürfnisse von Großunternehmen ausgerichtet sind und sich aus Kosten- und Kapazitätsgründen nicht vollständig auf KMUs (Klein- und Mittelbetriebe) übertragen lassen. Der Beitrag stellt deshalb ein Organisationsmodell, den sogenannten Lean Ramp-up vor, das speziell für den effizienten Serienanlauf in KMUs entwickelt wurde. Dem Lean Ramp-up liegt ein sozio-technisches Zielsystem zugrunde, das anstrebt die Beziehungen zwischen Mensch und Technik mit einem Organisationsmodell in Einklang zu bringen, um einen effizienten Serienanlauf zu erreichen. Der Beitrag leitet drei Gestaltungsregeln ab, die sich mit der Klassifikation von Serienanläufen, der organisatorischen Verankerung des Ordnungsrahmens mit Methoden und Werkzeugen und der systematischen Auswahl von Methoden und Werkzeugen mithilfe des QFD (Quality Function Deployment) befassen. Anschließend wird anhand eines Projektbeispiels die Umsetzung des Lean Ramp-up bei einem mittelständischen Landmaschinenhersteller beschrieben. Das Projekt verfolgte das Ziel, Qualität, Produktivität und Termintreue für den Serienanlauf einer Rundballenpresse zu verbessern. Im Vergleich mit dem Vorjahr konnte der Anstieg der Anlaufkurve um circa 85 \% erhöht und die Anlaufzeit um über 77 \% reduziert werden, sodass die Termin- und Produktivitätsziele erreicht wurden.},
  File                     = {:../10_research/bibliography/[Dombrowski2009] - Lean Ramp-Up - Ein Organisationsmodell.pdf:PDF},
  ISSN                     = {ISSN 0947-0085},
  Keywords                 = {KMU (Klein- und Mittelbetriebe)},
  Owner                    = {rudolph},
  Review                   = {Scan replaced on 21.02.18
TUB QA18800
Lit: [Kuhn-2002] Laick-2003 [Schuh-2008a] Ohno-1993},
  Timestamp                = {2017.11.28}
